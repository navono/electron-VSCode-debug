{
  "name": "electron-VSCode-debug",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env PORT=3000 NODE_ENV=development node server.js",
    "vs-debug": "yarn dev --debug",
    "electron": "cross-env NODE_ENV=development electron ./app/main/index.js",
    "build": "yarn clean && yarn build:main && yarn build:renderer",
    "build:main": "webpack --config ./webpack.config.main.prod.js --progress --profile --colors",
    "build:renderer": "webpack --config ./webpack.config.renderer.prod.js --progress --profile --colors",
    "clean": "rimraf dist"
  },
  "repository": "https://github.com/navono/electron-VSCode-debug",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "navono",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/react": "^16.3.9",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.1.4",
    "electron": "^1.8.4",
    "electron-devtools-installer": "^2.2.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.0.1",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
